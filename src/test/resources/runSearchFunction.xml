<?xml version="1.0" encoding="UTF-8"?>
<suite parallel="false" thread-count="2" name="Run Search Feature">

    <listeners>
        <!--        <listener class-name="reportConfig.ExtentTestListener"/>-->
        <listener class-name="reportConfig.AllureTestListener"/>
        <listener class-name="reportConfig.MethodListener"/>
    </listeners>

    <!-- Global-->
    <parameter name="envName" value="local"/>
    <parameter name="serverName" value="dev"/>

    <test name="Run on CHROME">
        <parameter name="browser" value="h_chrome"/>
        <classes>
            <class name="com.usernopcommerce.register.Register_Success_Global"/>
            <class name="com.usernopcommerce.search.SearchFunction"/>
        </classes>
    </test>


</suite>